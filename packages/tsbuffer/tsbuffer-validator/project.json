{
  "name": "tsbuffer-tsbuffer-validator",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/tsbuffer/tsbuffer-validator/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/tsbuffer/tsbuffer-validator",
        "main": "packages/tsbuffer/tsbuffer-validator/src/index.ts",
        "tsConfig": "packages/tsbuffer/tsbuffer-validator/tsconfig.lib.json",
        "assets": []
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs tsbuffer-tsbuffer-validator {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/tsbuffer/tsbuffer-validator/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/tsbuffer/tsbuffer-validator/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["tsbuffer"]
}
